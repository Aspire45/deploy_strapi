{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "introduction": {
      "type": "richtext",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "technologies": {
      "type": "component",
      "repeatable": true,
      "component": "technologies.technologies"
    },
    "clientURLLink": {
      "type": "string",
      "required": true
    },
    "clientURLName": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date"
    },
    "smallTitle": {
      "type": "string",
      "required": true
    },
    "largeTitle": {
      "type": "string",
      "required": false
    },
    "categories": {
      "type": "component",
      "repeatable": true,
      "component": "categories.categories"
    },
    "tags": {
      "type": "component",
      "repeatable": true,
      "component": "tags.tags"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "mainImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "projectdesc": {
      "type": "string"
    }
  }
}
